.carousel{id: tag}
  - header_name = 'carousel_header ' + tag
  %h3{:class => header_name}
    = tag
  - if type == 'vendor'
    - if list
      - list.each do |vendor|
        - if vendor.picture != ''
          = render 'carousel_vendor_element', vendor: vendor, ownership: tag
              
  - if type == 'product'
    - if list
      - list.each do |product|
        - if product.picture != ''
          = render 'carousel_product_element', product: product, tag: tag

