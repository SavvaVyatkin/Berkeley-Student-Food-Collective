%div{:class => "container carousel_element #{ownership}", :id => vendor.name.gsub(' ', '_'), :onclick => "routeToPageDirectly('#{vendor_path(vendor.id)}')"}
  %button
    = image_tag vendor.picture, :class => 'image'
  %p.discovery-centered
    %td
      = vendor.name
  %p.overlay
    %td
      = 'Click here to learn more...'
